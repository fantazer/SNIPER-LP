	.section.section--gray
		.main-cont
			.section-title(data-aos="fade-in" data-aos-delay="200" data-aos-duration="1000") Кейсы
			.slider-wrap.slider-wrap--simple(data-aos="fade-up" data-aos-delay="600" data-aos-duration="1000")
				.slider-control.slider-control--left
					+icon("arrow-conner-left","icon")
				.slider-control.slider-control--right
					+icon("arrow-conner-right","icon")
				.slider-item
					- for (var i=0;i<5;i++)
						.slider-el-wrap
							.slider-el
								.option
									.option__header(style="background-image:url('img/triange-bg.png')")
										.option__img
											img(src="img/option-1.png", alt="")
										.option__title Компания по продаже женского белья для беременных и кормящих мам
									.option-wrap
										.option-col
											.option-col-head
												.option-col__title Инструменты
													+icon("plus","icon")
											.option-descr
												.list
													.list__el SEO
													.list__el  Яндекс.Директ
													.list__el  Google Ads
													.list__el  SMM
													.list__el  МойСклад
													.list__el  СДЭК
										.option-col
											.option-col-head
