include block_html/_style.pug
include block_html/_include.pug

.page
	+header()
	.content
		.main-section(style="background-image:url('img/bg-mainsection.png')")
			.header-wrap
				.main-cont
					.header(data-aos="fade-down" data-aos-delay="200" data-aos-duration="1000")
						a(href="/").logo
							img(src="img/logo.png", alt="")
						a(href="tel:8 000 000 00 00").header__phone
							+icon("call","icon")
							span 8 000 000 00 00
						.soc-row.soc-row--white
							a(href="#").soc-row__el
								+icon("soc-telegram","icon")
							a(href="#").soc-row__el
								+icon("soc-whatsapp","icon")
						div(data-modal="order").modal-get.btn--md.btn--yellow.header-get Получить консультацию
			.main-cont.main-cont--full
				.slogan-wrap
					.slogan(data-aos="fade-left" data-aos-delay="900" data-aos-duration="1000")
						.slogan__title Скидка до
							.slogan__title-cost 35000<span>рублей </span>
							| ежемесячно
						.slogan__text на комплексное продвижение.
						div(data-modal="order").modal-get.btn--md.btn--yellow.slogan__btn Получить консультацию
		.section-work(style="background-image:url('img/bg-triangle.png')")
			.main-cont
				.work-head
					.work-head__title(data-aos="fade-in" data-aos-delay="200" data-aos-duration="1000") Работаем по технологии комплексного продвижения craft MARKETING
					.work-head__get(data-aos="fade-left" data-aos-delay="900" data-aos-duration="1000")
						.soc-row
							a(href="#").soc-row__el
								+icon("soc-telegram","icon")
							a(href="#").soc-row__el
								+icon("soc-whatsapp","icon")
						div(data-modal="order").modal-get.btn--md.btn--mark.btn--mobile Онлайн консультация
				.work-cont(data-aos="fade-in" data-aos-delay="200" data-aos-duration="1000")
					.work-cont__info Знаем текущую потребность бизнеса, разработаем стратегию для достижения результата.
				.work-advant
					.work-advant__el(data-aos="fade-up" data-aos-delay="200" data-aos-duration="1000")
						+icon("money","icon")
						.work-advant__el-info
							b Меньше
							span тратьте
					.work-advant__el(data-aos="fade-up" data-aos-delay="500" data-aos-duration="1000")
						+icon("grow","icon")
						.work-advant__el-info
							b Больше
							span зарабатывайте
					.work-advant__el(data-aos="fade-up" data-aos-delay="800" data-aos-duration="1000")
						+icon("map","icon")
						.work-advant__el-info
							b Масштабируйте
							span свой бизнес
		.section-process
			.main-cont
				.process-head(data-aos="fade-in" data-aos-delay="200" data-aos-duration="1000")
					.process__title Как мы работаем
					.process-get
						div(data-modal="order").modal-get.btn--md.btn--mark.btn--mobile Хочу стратегию
						.soc-row
							a(href="#").soc-row__el
								+icon("soc-telegram","icon")
							a(href="#").soc-row__el
								+icon("soc-whatsapp","icon")
				.process-img(data-aos="fade-up" data-aos-delay="500" data-aos-duration="1000")
					img(src="img/arrow-number.png", alt="")
				.process
					.process__col(data-aos="fade-up" data-aos-delay="200" data-aos-duration="1000")
						.process__col-title 
							.process__col-title-numb
								img(src="img/number-1.png", alt="")
							.process__col-title-text Разработка стратегии
						.process__col-info
							ul
								li Определение целей и задач
								li Оценка рынка и конкурентов
								li Оценка интеграций
								li Аудит контекстной рекламы, SEO, SMM, репутации
								li Веб‑аналитика - метрики, цели, юзабилити
					.process__col(data-aos="fade-up" data-aos-delay="400" data-aos-duration="1000")
						.process__col-title
							.process__col-title-numb
								img(src="img/number-2.png", alt="")
							.process__col-title-text Горячий и отложенный спрос
						.process__col-info
							ul
								li Контекстная реклама
								li Таргетированная реклама
								li Таргетированная реклама в социальных сетях
								li Яндекс.Маркет и Google Покупки
								li Поисковое продвижение
								li Smart targeting - пол, возраст, ГЕО, доход, интерес
					.process__col(data-aos="fade-up" data-aos-delay="600" data-aos-duration="1000")
						.process__col-title
							.process__col-title-numb
								img(src="img/number-3.png", alt="")
							.process__col-title-text Принятие решения о покупке
						.process__col-info
							ul
								li Ретаргетинг
								li Ремаркетинг
								li Управление репутацией
								li Интернет Брендинг
								li Наличие и доставка
								li Онлайн оплата
					.process__col(data-aos="fade-up" data-aos-delay="800" data-aos-duration="1000")
						.process__col-title
							.process__col-title-numb
								img(src="img/number-4.png", alt="")
							.process__col-title-text Повторные покупки
						.process__col-info
							ul
								li Ведение соц.сетей
								li СМС рассылка
								li CRM маркетинг
					.process__col(data-aos="fade-up" data-aos-delay="1000" data-aos-duration="1000")
						.process__col-title
							.process__col-title-numb
								img(src="img/number-5.png", alt="")
							.process__col-title-text Отдел продаж
						.process__col-info
							ul
								li Ведение соц.сетей
								li СМС рассылка
								li CRM маркетинг
	.section.section--gray
		.main-cont
			.section-title(data-aos="fade-in" data-aos-delay="200" data-aos-duration="1000") Технологии
