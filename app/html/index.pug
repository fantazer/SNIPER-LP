											.option-descr
												.list
													.list__el Разработали интернет-магазин
													.list__el Запустили комплексное продвижение
										.option-col
											.option-col-head
												.option-col__title Что сделали
													+icon("plus","icon")
											.option-descr
												.list
													.list__el Разработали интернет-магазин
													.list__el Запустили комплексное продвижение
													.list__el <b>Стоимость лида:</b> 300 рублей.
													.list__el <b>Колличество привлеченных лидов:</b> +627
													.list__el <b>Колличество лидов в месяц:</b> +105
